

.topnav {
  padding: 0 5%;
  width: 90%;
  border: 1rpx solid rgba(0, 0, 0, 0.1);
  border-left: 0;
  border-right: 0;
  background-color: #fff;
}

.topnav view {
  height: 80rpx;
  flex: 1;
  display: flex;
  flex-direction: row;
  vertical-align: middle;
}

.topnav view image {
  height: 40rpx;
  width: 40rpx;
  margin-top: 20rpx;
}

.topnav view view {
  display: inline-block;
  line-height: 80rpx;
  vertical-align: top;
}

.communityName {
  font-size: 28rpx;
  margin-left: 5rpx;
  color: #888;
}

.news {
  padding-left: 20rpx;
}

.notice {
  padding: 0 5%;
  padding-top: 10px;
  padding-bottom: 10px;
  width: 90%;
  background-color: #fff;
  margin-bottom: 1px;
}

.notice image {
  height: 40rpx;
  width: 40rpx;
}

.notice .noticetxt {
