.weui-grids{
  background-color: #fff;
   overflow: hidden;
  width: 98%;
  margin-left: 1%;
  margin-right: 1%;
}
.weui-grids:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height:1rpx;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}


.weui-grids:after {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  width: 1rpx;
  bottom: 0;
  color: #f1f4f9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
}
.weui-grid1{
  position: relative;
float: left;
padding: 20px 10px;
width: 20%;
box-sizing: border-box;

}
.weui-grid1{
  display: flex;
  flex: 1;
  height: 150rpx;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
}
.weui-grid1:before {
  content: " ";
  position: absolute;
  right: 0;
  top: 0;
  width: 1rpx;
  bottom: 0;
  border-right: 1rpx solid #f1f4f9;
  color: #f1f4f9;
  -webkit-transform-origin: 100% 0;
  transform-origin: 100% 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
}

.weui-grid1:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1rpx;
  border-bottom: 1rpx solid #f1f4f9;
  color: #f1f4f9;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.weui-grid__bg {
  display: flex;
  flex: 1;
  flex-direction: column;
  padding: 0px 0px;
  box-sizing: border-box;
}

.weui-grid__bg image {
  display: block;
  width: 100%;
  height: 100%;
}


