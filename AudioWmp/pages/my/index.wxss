}

.btn_nav {
  width: 100%;
  padding-top: 10px;
  height: 90px;
  background-color: #fff;
}

.index_list {
  padding: 10px 5%;
}

.item {
  background-color: #fff;
  padding: 10px 10px 10px 10px;
  border-radius: 10px;
  margin-top: 10px;
}

.item .discount {
  padding: 7px 5px 5px 0;
  flex: 1;
  display: flex;
  flex-direction: row;
  border: 1rpx solid rgba(0, 0, 0, 0.1);
  border-left: 0;
  border-right: 0;
}

.item .discount view {
  line-height: 60px;
  font-size: 60rpx;
  width: 70%;
}

.item .btn {
  vertical-align: middle;
}

.synopsis {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  padding-top: 10rpx;
  padding-right: 10rpx;
}

.cTxtindex {
  font-size: 30rpx;
  color: #888;
  padding-top: 20rpx;
}

.CouponTitle {
